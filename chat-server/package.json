{
  "name": "docs-chat-server",
  "version": "1.0.0",
  "description": "Q&A server for the MongoDB Docs AI chatbot.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist/ && tsc",
    "dev": "nodemon src/index.ts",
    "format": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json,md}'",
    "start": "node ./dist/index.js",
    "test": "jest --forceExit",
    "seed-content:dev": "ts-node seed-content .env",
    "seed-content:test": "ts-node seed-content .env.test",
    "seed-content:staging": "ts-node seed-content .env.staging"
  },
  "engines": {
    "node": "^18",
    "npm": "8.*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.2",
    "common-tags": "^1.8.2",
    "chat-core": "file:../chat-core",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gpt3-tokenizer": "^1.1.5",
    "langchain": "^0.0.95",
    "openai": "^3.2.1",
    "stream-json": "^1.8.0",
    "winston": "^3.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/common-tags": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/stream-json": "^1.7.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
