{
  "name": "benchmarks",
  "version": "0.0.0",
  "description": "Benchmarks for evaluating how models perform on MongoDB-related tasks.",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [],
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "exports": "./build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "release": "release-it",
    "lint": "eslint ./src --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix && prettier ./src --check --write",
    "textToDriver:makeDatabases": "npm run build && node ./build/textToDriver/makeDatabases/index.js",
    "prebenchmark": "npm run build",
    "benchmark": "node ./build/bin/mongoDbBenchmarkCli.js"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7",
    "@babel/types": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^12",
    "@types/node-fetch": "^2.6.4",
    "chatbot-server-mongodb-public": "*",
    "csv-parser": "^3.2.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^10.2.0",
    "prettier": "^2",
    "release-it": "^16.1.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.6",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@supercharge/promise-pool": "^3.2.0",
    "ai": "^4.2.10",
    "autoevals": "^0.0.129",
    "csv-writer": "^1.6.0",
    "dotenv": "^16",
    "mongodb-chatbot-server": "*",
    "mongodb-mcp-server": "^0.2.0",
    "mongodb-rag-core": "*",
    "mongodb-schema": "^12.2.0",
    "yaml": "^2.7.1",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  }
}