    "dotenv": "^16",
    "encodeurl": "^2.0.0",
    "html-to-text": "^9.0.5",
    "jsonl-parse-stringify": "^1.0.3",
    "mongodb": "^6.16.0",
    "node-fetch": "^2.7.0",
    "striptags": "^3.2.0",
    "turndown": "^7.2.0",
    "xml2js": "^0.6.2",
