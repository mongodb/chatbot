    "autoevals": "^0.0.130",
    "braintrust": "^0.2.4",
    "eslint": "^9.33.0",
    "release-it": "^16.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@supercharge/promise-pool": "^3.2.0",
