{
  "name": "mongodb-artifact-generator",
  "version": "0.0.0",
  "description": "Generates content artifacts for MongoDB Education.",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [],
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "files": [
    "build",
    "README.md"
  ],
  "bin": {
    "mongodb-ai": "./build/main.js"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc -b tsconfig.build.json",
    "postbuild": "chmod +x build/main.js",
    "watch": "tsc -b -w",
    "test": "jest --forceExit",
    "lint": "eslint ./src --ext ts,tsx,js,jsx --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix && prettier ./src --check --write",
    "parseDriversCsv": "npm run build && node ./build/main.js parseDriversCsv --config='./build/standardConfig.js'"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7",
    "@babel/types": "^7.22.5",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^12.20.55",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^29.5.0",
    "nock": "^13.3.2",
    "prettier": "^2",
    "release-it": "^16.1.5",
    "ts-jest": "^29.1.0",
    "typedoc": "^0",
    "typescript": "^5"
  },
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.6",
    "@octokit/rest": "^20.1.0",
    "@types/jira-client": "^7.1.9",
    "common-tags": "^1.8.2",
    "dotenv": "^16",
    "front-matter": "^4.0.2",
    "jira-client": "^8.2.2",
    "mongodb-rag-core": "^0.1.0",
    "papaparse": "^5.4.1",
    "yaml": "^2.3.1",
    "yargs": "^17",
    "zod": "^3.22.4"
  }
}
