{
  "name": "mongodb-rag-ingest",
  "version": "0.1.10",
  "description": "MongoDB Ingest CLI for the MongoDB Chatbot Framework.",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [
    "retrieval-augmented-generation",
    "rag",
    "chatbot",
    "mongodb",
    "mongodb-chatbot-framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/chatbot.git"
  },
  "homepage": "https://mongodb.github.io/chatbot/ingest/configure/",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "files": [
    "build",
    "README.md"
  ],
  "exports": {
    ".": "./build/index.js",
    "./embed": "./build/embed/index.js",
    "./pages": "./build/pages/index.js",
    "./sources": "./build/sources/index.js",
    "./sources/snooty": "./build/sources/snooty/index.js",
    "./core": "./build/core/index.js"
  },
  "bin": {
    "ingest": "./build/main.js"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc -b tsconfig.build.json",
    "postbuild": "chmod +x build/main.js",
    "watch": "tsc -b -w",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --forceExit",
    "lint": "eslint ./src --ext js,jsx,ts,tsx --report-unused-disable-directives",
    "docs": "npx typedoc --excludePrivate --exclude '**/*.test.ts' --out docs src",
    "release": "release-it"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7",
    "@babel/types": "^7.22.5",
    "@types/deep-equal": "^1.0.1",
    "@types/encodeurl": "^1.0.0",
    "@types/html-to-text": "^9.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^12",
    "@types/node-fetch": "^2.6.4",
    "@types/turndown": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "chmod": "^0.2.1",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^29.5.0",
    "jsonl-parse-stringify": "^1.0.3",
    "mongodb-memory-server": "^8.13.0",
    "nock": "^13.3.2",
    "prettier": "^2",
    "release-it": "^16.1.5",
    "ts-jest": "^29.1.0",
    "typedoc": "^0",
    "typescript": "^5"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@azure/openai": "^1.0.0-beta.6",
    "@release-it/bumper": "^5.1.0",
    "acquit": "^1.3.0",
    "acquit-require": "^0.1.1",
    "deep-equal": "^2.2.1",
    "docdoctor": "^0.1.0",
    "dotenv": "^16",
    "encodeurl": "^1.0.2",
    "exponential-backoff": "^3.1.1",
    "front-matter": "^4.0.2",
    "gpt3-tokenizer": "^1.1.5",
    "html-to-text": "^9.0.5",
    "ignore": "^5.2.4",
    "jsdom": "^22.1.0",
    "langchain": "^0.1.13",
    "mongodb-rag-core": "*",
    "node-fetch": "^2",
    "rimraf": "^5.0.1",
    "simple-git": "^3.19.1",
    "turndown": "^7.1.2",
    "turndown-plugin-gfm": "^1.0.2",
    "yaml": "^2.3.1",
    "yargs": "^17"
  }
}
